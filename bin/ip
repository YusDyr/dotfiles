#!/usr/bin/env python

import socket
import struct
import sys

for line in sys.stdin:
    try:
        line = line.strip()
        if '.' in line:
            print(struct.unpack("!L", line)[0])
        else:
            print(socket.inet_ntoa(struct.pack('!L', long(line.strip()))))
    except:
        print(line.strip())

    sys.stdout.flush()

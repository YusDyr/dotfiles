parser = argparse.ArgumentParser(description = 'Print out a given pokemon by number')
parser.add_argument('-g', '--generation', nargs = '?', type = int, default = 0, help = 'use a specific local pokedex (default = use national)')
parser.add_argument('number', type = int, help = 'pokedex number (generational if -g is specified, otherwise national)')
args = parser.parse_args()

for gen, local, national, name in pokemon:
    if (args.generation == 0 and args.number == national) or (args.generation == gen and args.number == local):
        print(name)
        exit()

parser.print_help()
